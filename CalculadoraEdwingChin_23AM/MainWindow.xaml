        <Button x:Name="ButtonSuma" Content="+" HorizontalAlignment="Left" Height="50" Margin="212,290,0,0" VerticalAlignment="Top" Width="52" Foreground="#FFEFE8E8" BorderBrush="Black" Background="#FFE68D20" FontSize="30"/>
        <Button x:Name="ButtonResta" Content="-" HorizontalAlignment="Left" Height="50" Margin="212,226,0,0" VerticalAlignment="Top" Width="52" Foreground="#FFEFE8E8" BorderBrush="Black" Background="#FFE68D20" FontSize="30"/>
        <Button x:Name="ButtonDivision" Content="÷" HorizontalAlignment="Left" Height="50" Margin="212,162,0,0" VerticalAlignment="Top" Width="52" Foreground="#FFEFE8E8" BorderBrush="Black" Background="#FFE68D20" FontSize="30"/>
